{
  "name": "@ecl/ecl-webcomponents",
  "version": "0.15.1",
  "description": "ECL web components",
  "license": "EUPL-1.2",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ecl-webcomponents/ecl-webcomponents.esm.js",
  "exports": {
    ".": {
     "import": "./dist/ecl-webcomponents/ecl-webcomponents.esm.js",
     "require": "./dist/ecl-webcomponents/ecl-webcomponents.cjs.js"
   },
   "./dist/*": {
     "import": "./dist/*",
     "types": "./dist/*"
   },
   "./components/*": {
     "import": "./dist/components/*.js",
     "types": "./dist/components/*.d.ts"
   },
   "./loader": {
     "import": "./loader/index.js",
     "require": "./loader/index.cjs",
     "types": "./loader/index.d.ts"
   }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ec-europa/ecl-webcomponents"
  },
  "dependencies": {
    "@stencil/core": "4.11.0"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "0.8.4",
    "@stencil/react-output-target": "0.5.3"
  }
}
